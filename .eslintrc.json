{
    "parser": "@babel/eslint-parser",
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "settings": {
        "import/resolver" : { "babel-module" : {}}
      },
      "globals": {
        "__rootdir": "readonly"
      },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": ["error"]
        
    }
}
