{
  "name": "dwpc2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "dev": "DEBUG=dwpc2:server && babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode development",
    "devb": "DEBUG=dwpcii1:server && babel-watch ./server/bin/www",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.19",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-watch": "^7.8.1",
    "core-js": "^3.32.2",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
